' ÷¡“‘' ====' ¡N OPEN-SOURCE TOOL FOR DRAWING 640X400 VECTOR ART ON THE MEGA65#OUTPUT "VART"' #DEFINE ƒ≈¬’«Ø”œ“‘=1' MENU SYSTEM' +------+--------+-----+------+----------+---------+------+------+------+-----+' ! LINE ! CIRCLE ! BOX ! POLY ! (X) FILL ! PALETTE ! LOAD ! SAVE ! UNDO ! REDO!' +------+--------+-----+------+----------+---------+------+------+------+-----+' X = EXIT' ? = SHOW/HIDE MENU' CTRL+1-8 = SELECT FROM 1ST 8 COLOURS' MEGA+1-8 = SELECT FROM 2ND 8 COLOURS' CTRL+D = CHOOSE FROM DEFAULT PALETTE' CTRL+A = CHOOSE FROM ALTERNATE PALETTE.DECLARES'--------#DECLARE KEY$#DECLARE K,M0%, M1%, A$#DECLARE X%, Y%, W%, H%, X2%, Y2%#DECLARE MX%, MY%, MB%, MBØOLD%#DECLARE MENUØSELECTION = -1#DECLARE RET%#DECLARE LASTØCOLOUR% = -1#DECLARE SELECTEDØCOLOUR% = 1#DECLARE OLDØMENUØSELECTION% = -1#DECLARE YMIN%, YMAX%, IDX%, PIDX%, PX%(20)#DECLARE REV%, Y0%, Y1%, X0%, X1%, TMP%, M, C, I%, J%, SWAPPED%#DECLARE LOADØSTATE% = -4#DECLARE FILLØSTATE%#DECLARE LINEØSTATE%, LX0%, LY0%#DECLARE CIRCLEØSTATE%, CX0%, CY0%, RADIUS%#DECLARE ELLIPSEØSTATE%, EX0%, EY0%, XRADIUS%, YRADIUS%#DECLARE BOXØSTATE%, BX0%, BY0%#DECLARE POLYØSTATE%, POLYØIDX%=0, POLYØX%(100), POLYØY%(100)#DECLARE VECØDATA%(3000)VECØDATA%(0) = ASC("V")  ' »≈¡ƒ≈“ ∆œ“ "÷≈√‘œ“" ƒ¡‘¡VECØDATA%(1) = ASC("E")VECØDATA%(2) = ASC("C")#DECLARE VECØSIZE% = 3#DECLARE STOR%(500), STORØCNT%#STRUCT ¬’‘‘œŒ X%, Y%, W%, H%, TYPE%, STATE%, TEXT$#DEFINE ‘Ÿ–≈ØŒœ“Õ¡Ã = 0#DEFINE ‘Ÿ–≈Ø‘œ««Ã≈ = 1#DEFINE Ã…Œ≈    = 0#DEFINE √…“√Ã≈  = 1#DEFINE ≈ÃÃ…–”≈ = 2#DEFINE ¬œÿ     = 3#DEFINE –œÃŸ    = 4#DEFINE ∆…ÃÃ    = 5#DEFINE ”¡÷≈    = 6#DEFINE Ãœ¡ƒ    = 7#DEFINE ≈ÿ…‘    = 8#DEFINE Õ¡ÿØ¬‘Œ = 9¬’‘‘œŒ BTNS(Õ¡ÿØ¬‘Œ) = [     _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "LINE" ],    _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "CIRCLE" ],  _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "ELLIPSE" ], _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "BOX" ],     _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "POLY" ],    _  [ 0, 0, 0, 0, ‘Ÿ–≈Ø‘œ««Ã≈, 0, "FILL" ],    _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "SAVE" ],    _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "LOAD" ],    _  [ 0, 0, 0, 0, ‘Ÿ–≈ØŒœ“Õ¡Ã, 0, "EXIT" ]     _] #DEFINE √œÃœ’“ = 63.INITØBTNS'---------BTNSØW%(0) = LEN(BTNSØTEXT$(0))+2BTNSØH%(0) = 3FOR K = 1 TO Õ¡ÿØ¬‘Œ-1  BTNSØX%(K) = BTNSØX%(K-1) + BTNSØW%(K-1)  BTNSØW%(K) = LEN(BTNSØTEXT$(K)) + 2  BTNSØH%(K) = 3NEXT K.MAIN'----  GOSUB INIT     GOSUB DRAWØMENUØSYSTEM  GOSUB CHECKØINPUT  GOSUB ENDER  END.INIT'----  SCREEN 0,640,400,4  FOR K = 0 TO 62    READ X%    POKE $0600+K, X%  NEXT K  MOUSE ON, 1  SPRITE 0,1,1  RETURN#DEFINE √»“Ø»≈…«»‘ = 2#DEFINE √»“Ø◊…ƒ‘» = 1#DEFINE √»“Øƒ…“Ø’– = 1#DEFINE √»“Øƒ…“Ø“…«»‘ = 2#DEFINE √»“Øƒ…“Øƒœ◊Œ = 4#DEFINE √»“Øƒ…“ØÃ≈∆‘ = 8.CLEARØMENU'----------  FOR K=0 TO 7    EDMA 3, 640*2, 0, $40000+K*$4000  NEXT K  RETURN.DRAWØMENUØSYSTEM'----------------  GOSUB CLEARØMENU  PEN 0,1  ' DRAW ITEMS  FOR K = 0 TO Õ¡ÿØ¬‘Œ-1    X%=BTNSØX%(K)    Y%=BTNSØY%(K)    W%=(X% + BTNSØW%(K))    H%=(Y% + BTNSØH%(K))    PEN 0, 1    IF MENUØSELECTION=K AND BTNSØTYPE%(K) <> ‘Ÿ–≈Ø‘œ««Ã≈ THEN BEGIN      BOX X%*8, Y%*8, W%*8, H%*8, 1      PEN 0, 0    BEND:ELSE BEGIN      BOX X%*8, Y%*8, W%*8, H%*8      IF BTNSØTYPE%(K) = ‘Ÿ–≈Ø‘œ««Ã≈ AND BTNSØSTATE%(K)<>0 THEN BEGIN        PEN 0,3        BOX X%*8, Y%*8, W%*8, H%*8, 1        PEN 0,0      BEND    BEND    CHAR X%+1, Y%*8+4, √»“Ø»≈…«»‘, √»“Ø◊…ƒ‘», √»“Øƒ…“Ø“…«»‘, BTNSØTEXT$(K)    PEN 0, 1  NEXT K  PEN 0,SELECTEDØCOLOUR% : SPRITE 0,1,SELECTEDØCOLOUR%  RETURN.STOREØCOLOUR'------------  IF LASTØCOLOUR% <> SELECTEDØCOLOUR% THEN BEGIN    VECØDATA%(VECØSIZE%) = √œÃœ’“ : VECØSIZE% = VECØSIZE% + 1    VECØDATA%(VECØSIZE%) = SELECTEDØCOLOUR% : VECØSIZE% = VECØSIZE% + 1    LASTØCOLOUR% = SELECTEDØCOLOUR%  BEND  RETURN.STORE'-----  FOR K = 0 TO STORØCNT%    VECØDATA%(VECØSIZE%) = STOR%(K)    VECØSIZE% = VECØSIZE% + 1  NEXT K  RETURN.STOREØLINE'----------  STOR%(0) = Ã…Œ≈ + 64 * BTNSØSTATE%(∆…ÃÃ)  STOR%(1) = LX0% AND 255 : STOR%(2) = LX0% >> 8  STOR%(3) = LY0% AND 255 : STOR%(4) = LY0% >> 8  STOR%(5) = MX% AND 255 : STOR%(6) = MX% >> 8  STOR%(7) = MY% AND 255 : STOR%(8) = MY% >> 8  STORØCNT% = 8 : GOSUB STORE  RETURN.ONØCLICKØLINE'-------------  IF LINEØSTATE% = 0 THEN BEGIN    GOSUB STOREØCOLOUR    LX0%=MX%    LY0%=MY%    DOT MX%,MY%    LINEØSTATE%=1  BEND:ELSE IF LINEØSTATE% = 1 THEN BEGIN    LINE LX0%, LY0%, MX%, MY%    GOSUB STOREØLINE    LINEØSTATE% = 0  BEND  RETURN.ONØCLICKØCIRCLE'---------------  IF CIRCLEØSTATE% = 0 THEN BEGIN    GOSUB STOREØCOLOUR    CX0%=MX%    CY0%=MY%    STOR%(0) = √…“√Ã≈ + 64 * BTNSØSTATE%(∆…ÃÃ)    STOR%(1) = CX0% AND 255 : STOR%(2) = CX0% >> 8    STOR%(3) = CY0% AND 255 : STOR%(4) = CY0% >> 8    CIRCLEØSTATE%=1  BEND:ELSE IF CIRCLEØSTATE% = 1 THEN BEGIN    RADIUS% = SQR( (MX%-CX0%)^2 + (MY%-CY0%)^2 )    CIRCLE CX0%, CY0%, RADIUS%, BTNSØSTATE%(∆…ÃÃ)    STOR%(5) = RADIUS% AND 255 : STOR%(6) = RADIUS% >> 8    STORØCNT% = 6 : GOSUB STORE    CIRCLEØSTATE% = 0  BEND  RETURN.ONØCLICKØELLIPSE'----------------  IF ELLIPSEØSTATE% = 0 THEN BEGIN    GOSUB STOREØCOLOUR    EX0%=MX%    EY0%=MY%    STOR%(0) = ≈ÃÃ…–”≈ + 64 * BTNSØSTATE%(∆…ÃÃ)    STOR%(1) = EX0% AND 255 : STOR%(2) = EX0% >> 8    STOR%(3) = EY0% AND 255 : STOR%(4) = EY0% >> 8    ELLIPSEØSTATE%=1    BEND:ELSE IF ELLIPSEØSTATE% = 1 THEN BEGIN    XRADIUS% = ABS(MX%-EX0%)    YRADIUS% = ABS(MY%-EY0%)    ELLIPSE EX0%, EY0%, XRADIUS%, YRADIUS%, BTNSØSTATE%(∆…ÃÃ)    STOR%(5) = XRADIUS% AND 255 : STOR%(6) = XRADIUS% >> 8    STOR%(7) = XRADIUS% AND 255 : STOR%(8) = XRADIUS% >> 8    STORØCNT% = 8 : GOSUB STORE    ELLIPSEØSTATE% = 0  BEND  RETURN.ONØCLICKØBOX'------------  IF BOXØSTATE% = 0 THEN BEGIN    GOSUB STOREØCOLOUR    BX0%=MX%    BY0%=MY%        STOR%(0) = ¬œÿ + 64 * BTNSØSTATE%(∆…ÃÃ)    STOR%(1) = BX0% AND 255 : STOR%(2) = BX0% >> 8    STOR%(3) = BY0% AND 255 : STOR%(4) = BY0% >> 8    BOXØSTATE%=1  BEND: ELSE IF BOXØSTATE% = 1 THEN BEGIN    BOX BX0%, BY0%, MX%, MY%, BTNSØSTATE%(∆…ÃÃ)    STOR%(5) = MX% AND 255 : STOR%(6) = MX% >> 8    STOR%(7) = MY% AND 255 : STOR%(8) = MY% >> 8    STORØCNT% = 8 : GOSUB STORE    BOXØSTATE% = 0  BEND  RETURN.FINDØYMINØYMAX'--------------  YMIN% = POLYØY%(0)  YMAX% = POLYØY%(0)  FOR IDX% = 0 TO POLYØIDX%-1    IF POLYØY%(IDX%) < YMIN% THEN YMIN% = POLYØY%(IDX%)    IF POLYØY%(IDX%) > YMAX% THEN YMAX% = POLYØY%(IDX%)  NEXT IDX%  RETURN .ISØYØINØLINEØYRANGE'-------------------  RET% = 0: REV% = 0  Y0% = POLYØY%(IDX%-1)  Y1% = POLYØY%(IDX%)  IF Y0% > Y1% THEN TMP%=Y0%:Y0% = Y1%:Y1% = TMP%:REV%=1  IF Y0% <= Y% AND Y% < Y1% THEN RET% = 1  RETURN.CHECKØLINEØINTERSECT'--------------------  GOSUB ISØYØINØLINEØYRANGE  IF RET% = 0 THEN RETURN  X0% = POLYØX%(IDX%-1)  X1% = POLYØX%(IDX%)  IF REV%=1 THEN TMP%=X0%:X0% = X1%:X1% = TMP%  IF X0% = X1% THEN X% =X0%:RET% = 1:RETURN  M = (Y1%-Y0%) / (X1% - X0%)  C = Y0% - M * X0%    X% = (Y% - C) / M  RET% = 1  RETURN  .FINDØINTERSECTS'---------------  PIDX% = 0  ' CHECK LAST-POINT TO FIRST  POLYØX%(POLYØIDX%) = POLYØX%(0)  POLYØY%(POLYØIDX%) = POLYØY%(0)  FOR IDX% = 1 TO POLYØIDX%    GOSUB CHECKØLINEØINTERSECT    IF RET% = 1 THEN PX%(PIDX%)=X% : PIDX% = PIDX% + 1  NEXT IDX%  RETURN.SORTØINTERSECTS'---------------  ' BUBBLE SORT ALGORITHM FROM: GEEKSFORGEEKS.ORG/BUBBLE-SORT  #IFDEF ƒ≈¬’«Ø”œ“‘  GOSUB CLEARØMENU  CHAR 0,0,1,1,2,STR$(PIDX%-1)  FOR I%=0 TO PIDX%-1    CHAR 4+I%*4,0,1,1,2,STR$(PX%(I%))  NEXT I%#ENDIF  FOR I% = 0 TO PIDX%-2    SWAPPED% = 0    FOR J% = 0 TO PIDX%-I%-2      IF PX%(J%) > PX%(J%+1) THEN BEGIN        TMP%=PX%(J%)        PX%(J%) = PX%(J%+1)        PX%(J%+1) = TMP%        SWAPPED% = 1      BEND    NEXT J%    IF SWAPPED% = 0 THEN I% = PIDX%  ' EXIT EARLY  NEXT I%#IFDEF ƒ≈¬’«Ø”œ“‘  FOR I%=0 TO PIDX%-1    CHAR 4+I%*4,10,1,1,2,STR$(PX%(I%))  NEXT I%  GETKEY A$#ENDIF  RETURN.DRAWØHORIZONTALØINTERSECTS'--------------------------  GOSUB FINDØINTERSECTS  GOSUB SORTØINTERSECTS  FOR IDX%=0 TO PIDX%-1 STEP 2    LINE PX%(IDX%),Y%,PX%(IDX%+1),Y%  NEXT IDX%  RETURN.FILLØPOLY'---------  GOSUB FINDØYMINØYMAX  FOR Y%=YMIN% TO YMAX%    GOSUB DRAWØHORIZONTALØINTERSECTS  NEXT Y%  RETURN.ONØCLICKØPOLY'-------------  IF POLYØSTATE% = 0 THEN BEGIN    GOSUB STOREØCOLOUR    STOR%(0) = –œÃŸ + 64 * BTNSØSTATE%(∆…ÃÃ)    STOR%(1) = 0 ' PLACEHOLDER FOR NUMBER OF POINTS    STORØCNT% = 1    POLYØSTATE% = 1    POLYØIDX% = 0  BEND    IF POLYØIDX% > 0 THEN BEGIN    IF POLYØX%(POLYØIDX%-1)=MX% AND POLYØY%(POLYØIDX%-1)=MY% THEN BEGIN      LINE MX%,MY%,POLYØX%(0),POLYØY%(0)      STOR%(1) = POLYØIDX%      GOSUB STORE      POLYØSTATE% = 0      IF BTNSØSTATE%(∆…ÃÃ) = 1 THEN GOSUB FILLØPOLY      RETURN    BEND  BEND  POLYØX%(POLYØIDX%) = MX%  POLYØY%(POLYØIDX%) = MY%  STORØCNT%=STORØCNT%+1 : STOR%(STORØCNT%) = MX% AND 255  STORØCNT%=STORØCNT%+1 : STOR%(STORØCNT%) = MX% >> 8  STORØCNT%=STORØCNT%+1 : STOR%(STORØCNT%) = MY% AND 255  STORØCNT%=STORØCNT%+1 : STOR%(STORØCNT%) = MY% >> 8  IF POLYØIDX% = 0 THEN BEGIN    DOT MX%, MY%  BEND:ELSE BEGIN    LINE POLYØX%(POLYØIDX%-1), POLYØY%(POLYØIDX%-1), MX%, MY%  BEND  POLYØIDX% = POLYØIDX% + 1  RETURN.ONØCLICKØFILL'-------------  IF BTNSØSTATE%(∆…ÃÃ) = 0 THEN BTNSØSTATE%(∆…ÃÃ)=1:ELSE BTNSØSTATE%(∆…ÃÃ)=0  RETURN.ONØCLICKØSAVE'-------------  BORDER 3  ' FOR NOW, JUST SAVE TO A SINGLE HARD-CODED FILE  ^^DOPEN #2,"@TEST.V",W,U8  FOR K = 0 TO VECØSIZE%-1    PRINT #2, CHR$(VECØDATA%(K));  NEXT K  DCLOSE #2  RETURN.LOADØERROR'----------  BORDER 2  LOADØSTATE% = -4  RETURN.CHECKØVECØHEADER'----------------  RET% = 0  IF K<>ASC("V") THEN RETURN  GET#2, K:PRINT K;  IF K<>ASC("E") THEN RETURN  GET#2, K:PRINT K;  IF K<>ASC("C") THEN RETURN  RET% = 1  VECØSIZE% = 3  LOADØSTATE%=-1  RETURN.ONØCLICKØLOAD'-------------  ' FOR NOW, JUST LOAD FROM A SINGLE HARD-CODED FILE  LOADØSTATE% = -4  ^^DOPEN #2,"TEST.V",R,U8  DO    GET#2, K:PRINT "RD=";K;    IF ST THEN EXIT    IF LOADØSTATE% = -4 THEN BEGIN      PRINT "CHECKØVEC ";      GOSUB CHECKØVECØHEADER      IF RET% = 0 THEN GOSUB LOADØERROR:EXIT:ELSE GOTO NXTØLP    BEND    IF LOADØSTATE% = -1 THEN BEGIN      PRINT "NEWØCMD ";      FILLØSTATE% = (K AND 64)/64      K = K AND 63      LOADØSTATE% = K    BEND    IF LOADØSTATE% = √œÃœ’“ THEN BEGIN      PRINT "COLOUR ";      GET#2,K:PRINT K;      PEN 0, K      VECØDATA%(VECØSIZE%) = √œÃœ’“      VECØSIZE% = VECØSIZE% +1      VECØDATA%(VECØSIZE%) = K      VECØSIZE% = VECØSIZE% +1      LASTØCOLOUR% = K      SELECTEDØCOLOUR% = K      LOADØSTATE% = -1      GOTO NXTØLP    BEND    IF LOADØSTATE% = Ã…Œ≈ THEN BEGIN      PRINT "LINE ";      GET#2,M0% : M0% = M0% AND 255 : PRINT M0%;      GET#2,M1% : M1% = M1% AND 255 : PRINT M1%;      LX0% = M0% + (M1% << 8)      GET#2,M0% : M0% = M0% AND 255 : PRINT M0%;      GET#2,M1% : M1% = M1% AND 255 : PRINT M1%;      LY0% = M0% + (M1% << 8)      GET#2,M0% : M0% = M0% AND 255 : PRINT M0%;      GET#2,M1% : M1% = M1% AND 255 : PRINT M1%;      MX% = M0% + (M1% << 8)      GET#2,M0% : M0% = M0% AND 255 : PRINT M0%;      GET#2,M1% : M1% = M1% AND 255 : PRINT M1%;      MY% = M0% + (M1% << 8)      LINE LX0%, LY0%, MX%, MY%      GOSUB STOREØLINE      LOADØSTATE% = -1      GOTO NXTØLP    BEND.NXTØLP  LOOP  DCLOSE #2  RETURN.CHECKØACTIONØCLICK'------------------  IF MENUØSELECTION = Ã…Œ≈ THEN GOSUB ONØCLICKØLINE  IF MENUØSELECTION = √…“√Ã≈ THEN GOSUB ONØCLICKØCIRCLE  IF MENUØSELECTION = ≈ÃÃ…–”≈ THEN GOSUB ONØCLICKØELLIPSE  IF MENUØSELECTION = ¬œÿ THEN GOSUB ONØCLICKØBOX  IF MENUØSELECTION = –œÃŸ THEN GOSUB ONØCLICKØPOLY  RETURN.CHECKØCOLOURØKEYPRESS'---------------------  IF A$=CHR$(144) THEN SELECTEDØCOLOUR% = 0  IF A$=CHR$(5)   THEN SELECTEDØCOLOUR% = 1  IF A$=CHR$(28)  THEN SELECTEDØCOLOUR% = 2  IF A$=CHR$(159) THEN SELECTEDØCOLOUR% = 3  IF A$=CHR$(156) THEN SELECTEDØCOLOUR% = 4  IF A$=CHR$(30)  THEN SELECTEDØCOLOUR% = 5  IF A$=CHR$(31)  THEN SELECTEDØCOLOUR% = 6  IF A$=CHR$(158) THEN SELECTEDØCOLOUR% = 7  IF A$=CHR$(129) THEN SELECTEDØCOLOUR% = 8  IF A$=CHR$(149) THEN SELECTEDØCOLOUR% = 9  IF A$=CHR$(150) THEN SELECTEDØCOLOUR% = 10  IF A$=CHR$(151) THEN SELECTEDØCOLOUR% = 11  IF A$=CHR$(152) THEN SELECTEDØCOLOUR% = 12  IF A$=CHR$(153) THEN SELECTEDØCOLOUR% = 13  IF A$=CHR$(154) THEN SELECTEDØCOLOUR% = 14  IF A$=CHR$(155) THEN SELECTEDØCOLOUR% = 15  PEN 0,SELECTEDØCOLOUR% : SPRITE 0,1,SELECTEDØCOLOUR%  RETURN.CHECKØINPUT'-----------  GET A$  IF A$="X" THEN RETURN  GOSUB CHECKØCOLOURØKEYPRESS  RMOUSE MX%, MY%, MB%  MX%=(MX%-24)*2  MY%=(MY%-50)*2  IF MB%=128 AND MBØOLD%=0 THEN BEGIN    MBØOLD%=128    GOSUB CHECKØBUTTONØHIT        IF RET%=1 THEN BEGIN      IF MENUØSELECTION = ≈ÿ…‘ THEN SCREEN CLOSE:END    BEND:ELSE BEGIN      GOSUB CHECKØACTIONØCLICK    BEND  BEND  IF MBØOLD%=128 AND MB%=0 THEN MBØOLD% = 0  'DEBUG STUFF  'A$ = STR$(MX%) + ", " + STR$(MY%) + ", " + STR$(MB%)  'PEN 0, 0  'BOX 0, 64, 150, 80, 1  'PEN 0, 1  'CHAR 0, 64, √»“Ø»≈…«»‘, √»“Ø◊…ƒ‘», √»“Øƒ…“Ø“…«»‘, A$  GOTO CHECKØINPUT.CHECKØBUTTONØHIT'----------------  RET%=0  FOR K = 0 TO Õ¡ÿØ¬‘Œ-1    X%=BTNSØX%(K)*8    Y%=BTNSØY%(K)*8    W%=BTNSØW%(K)*8    H%=BTNSØH%(K)*8    X2%=X%+W%    Y2%=Y%+H%    IF X%<=MX% AND MX%<=X2% AND Y%<=MY% AND MY%<=Y2% THEN BEGIN      OLDØMENUØSELECTION% = MENUØSELECTION      MENUØSELECTION = K      IF MENUØSELECTION = ∆…ÃÃ THEN BEGIN        GOSUB ONØCLICKØFILL        MENUØSELECTION = OLDØMENUØSELECTION%      BEND: ELSE IF MENUØSELECTION = ”¡÷≈ THEN BEGIN        GOSUB ONØCLICKØSAVE      BEND: ELSE IF MENUØSELECTION = Ãœ¡ƒ THEN BEGIN        GOSUB ONØCLICKØLOAD      BEND      RET%=1      GOSUB DRAWØMENUØSYSTEM      K=Õ¡ÿØ¬‘Œ-1    BEND  NEXT K  RETURN  .ENDER'-----  SCREEN CLOSE  MOUSE OFF  RETURN.DATA'MOUSE POINTERDATA %111